                                <label class="btn btn-success" type="button">
                                    Seleccionar<input id="fileUltimoReciboSueldo" name="fileUltimoReciboSueldo" type="file" data-size-max="10" style="display:none;" accept="image/png,image/jpeg,application/pdf" onchange="fileUltimoRS();">
                                </label>
                            </span>
                        </div>
                        <div style="display: block;margin-top: 5px;margin-bottom: 10px;color: #737373;">
                            Tamaño máximo permitido 10MB
                        </div>
                    </div>
                </div>

                <br />
                <br />

                <fieldset>
                    <legend>Antecedentes</legend>
                </fieldset>
                <div class="row">
                    <div class="form-group has-feedback col-lg-4">
                        <div class="input-group ">
                            <div class="input-group-addon"><span class="glyphicon glyphicon-pencil"></span></div>
                            <input class="form-control" data-error="SucesoraAntecedente es un campo requerido." data-validate="false" id="SucesoraAntecedente" name="SucesoraAntecedente" placeholder="Sucesora" onchange="changeCampos()" type="text" autocomplete="off">
