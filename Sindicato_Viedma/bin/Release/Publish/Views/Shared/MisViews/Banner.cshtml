@{
    ViewData["Title"] = "Banner";
}

<div class="w3-display-container w3-grayscale-min" style="margin-bottom: 15px">
    <img src="~/images/bnnr-form-b.jpg" style="width:100%">

    <div class="w3-display-middle w3-center w3-hide-medium w3-hide-large">
        <img src="~/images/tmp/SEOCA-Logo.png">
    </div>

    <div class="w3-display-middle
                    w3-center
                    w3-container
                    w3-round-xxlarge
                    w3-light-grey
                    w3-hover-white
                    w3-hide-small"
         style="top:45%;
                    opacity:0.8;
                    width:70%">
        <br />

        <h2 class="w3-blue w3-round-xxlarge">
            <img src="~/images/tmp/seoca-nav.png" class="w3-margin-right middle">
            3er Millenium - Sistema de Aportes Sindicales <b>S.E.O.C.A</b>
        </h2>

        @if (ViewBag.Session != false)
        {
            <div class="w3-center w3-margin-bottom">
                <h4>Cuit: @ViewBag.CUITGLOBAL</h4>
                <div>
                    <div class="w3-left w3-margin-left">
                        <h4>Razon Social: @ViewBag.RazonSocialGLOBAL</h4>
                    </div>
                    <div class="w3-right w3-margin-right">
                        <h4>Nombre Fantasia: @ViewBag.NombreFantasiaGLOBAL</h4>
                    </div>
                </div>
                
            </div>
        }

    </div>

</div>