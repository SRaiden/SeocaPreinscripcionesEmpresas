@{
    ViewBag.Title = "Empresa";
}

<div class="container-100">
    <div class="wrap-100">
        <div class="form-100 validate-form">
            <span class="title-100-form"> Formulario de solicitud de preinscripcion de Empresas </span>
        </div>


        <h3 class="h3-input100"> Datos Empresariales</h3>
        <hr class="hrs" />
        <br />

        <div class="Datos">
            <div class="wrap-input100 validate-input bg1 alert-validate" data-validate="Please Type Your Name">
                <span class="label-cuit"> Cuit </span>
                <input type="number" id="Cuit" name="Cuit" class="input100" onchange="changeCampos()" placeholder="Ingrese su Cuit">
            </div>

            <div class="w3-row">
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Razon Social</span>
                    <input type="text" id="RazonSocial" name="RazonSocial" class="input100" onchange="changeCampos()" placeholder="Ingrese su Razon Social">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Nombre Fantasia</span>
                    <input type="text" id="NombreFantasia" name="NombreFantasia" class="input100" onchange="changeCampos()" placeholder="Ingrese su Nombre de Fantasia">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Domicilio Real</span>
                    <input type="text" id="DomicilioReal" name="DomicilioReal" class="input100" onchange="changeCampos()" placeholder="Ingrese su Direccion Real">
                </div>
            </div>

            <div class="w3-row">
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Localidad Real</span>
                    <select class="w3-select bg1" name="LocalidadReal" id="LocalidadReal">
                        <option value="0">< Seleccione una localidad ></option>
                        @{
                            if (ViewData["Localidades"] != null)
                            {
                                IEnumerable<dynamic> Localidades = (IEnumerable<dynamic>)ViewData["Localidades"];

                                foreach (dynamic Loc in Localidades)
                                {
                                    //string Descripcion = EmpAct.Descripcion;
                                    //int Id = EmpAct.Id;
                                    if (@Loc.Id != 0)
                                    {
                                        <option value="@Loc.Id">@Loc.Nombre_Localidad</option>
                                    }
                                }
                            }
                        }

                    </select>
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Telefono Real</span>
                    <input type="number" id="TelefonoReal" name="TelefonoReal" class="input100" onchange="changeCampos()" placeholder="Ingrese su Telefono Real">
                </div>
                <div class="w3-third">
                </div>
            </div>

            <div class="w3-row">
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Domicilio Legal</span>
                    <input type="text" id="DomicilioLegal" name="DomicilioLegal" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Localidad Legal</span>
                    <select class="w3-select bg1" name="LocalidadLegal" id="LocalidadLegal">
                        <option value="0">< Seleccione una localidad ></option>
                        @{
                            if (ViewData["Localidades"] != null)
                            {
                                IEnumerable<dynamic> Localidades = (IEnumerable<dynamic>)ViewData["Localidades"];

                                foreach (dynamic Loc in Localidades)
                                {
                                    //string Descripcion = EmpAct.Descripcion;
                                    //int Id = EmpAct.Id;
                                    if (@Loc.Id != 0)
                                    {
                                        <option value="@Loc.Id">@Loc.Nombre_Localidad</option>
                                    }
                                }
                            }
                        }

                    </select>
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Telefono Legal</span>
                    <input type="number" id="TelefonoLegal" name="TelefonoLegal" class="input100" onchange="changeCampos()">
                </div>
            </div>

            <div class="w3-row">
                <!-- -------------------- Fecha / Importe / Interes_Mora -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Actividad</span>
                    <select class="w3-select bg1" name="Actividad" id="Actividad">
                        <option value="0">< Seleccione una actividad ></option>
                        @{
                            if (ViewData["Empresas_Actividades"] != null)
                            {
                                IEnumerable<dynamic> Empresas_Actividades = (IEnumerable<dynamic>)ViewData["Empresas_Actividades"];

                                foreach (dynamic EmpAct in Empresas_Actividades)
                                {
                                    //string Descripcion = EmpAct.Descripcion;
                                    //int Id = EmpAct.Id;
                                    if (EmpAct.Id != 0)
                                    {
                                        <option value="@EmpAct.Id">@EmpAct.Descripcion</option>
                                    }
                                }
                            }
                        }
                    </select>
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Email</span>
                    <input type="email" id="Email" name="Email" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Pagina Web</span>
                    <input type="text" id="PaginaWeb" name="PaginaWeb" class="input100" onchange="changeCampos()">
                </div>
            </div>
        </div>

        <h3 class="h3-input100"> Titulares </h3>
        <hr class="hrs" />
        <br />

        <div class="titular">
            <div class="w3-row">
                <!-- -------------------- Apellido / Docimilio / Documento -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Apellido y Nombre</span>
                    <input type="text" id="ApellidoNombreTitular" name="ApellidoNombreTitular" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Domicilio Particular</span>
                    <input type="text" id="DomicilioParticularTitular" name="DomicilioParticularTitular" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Documento</span>
                    <input type="number" id="DocumentoTitular" name="DocumentoTitular" class="input100" onchange="changeCampos()">
                </div>
            </div>

            <div class="w3-row">
                <!-- -------------------- Apellido / Docimilio / Documento -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Cargo Empresa</span>
                    <input type="text" id="CargoEmpresaTitular" name="CargoEmpresaTitular" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third">
                </div>
                <div class="w3-third ">
                    <button class="w3-right w3-margin-top enviar w3-card bg-zul w3-text-white w3-hover-green w3-hover-border-cyan" type="button" onclick="insertarTitular()">
                        Agregar Titular
                    </button>
                </div>
                <br />
            </div>

            <div class="w3-row">
                <div class="container">
                    <table>
                        <thead>
                            <tr>
                                <th>Apellido Nombre</th>
                                <th>Domicilio</th>
                                <th>Documento</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody id="bodyTitular">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <br />
        <h3 class="h3-input100"> Empleados </h3>
        <hr class="hrs" />
        <br />

        <div class="empleado">
            <div class="w3-row">
                <!-- -------------------- Apellido / Docimilio / Documento -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Apellido y Nombre</span>
                    <input type="text" id="ApellidoNombreEmpleado" name="ApellidoNombreEmpleado" class="input100" onchange="changeCampos()">

                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Cuil Particular</span>
                    <input type="number" id="CuilEmpleado" name="CuilEmpleado" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Fecha Ingreso</span>
                    <input type="date" id="FechaIngresoEmpleado" name="FechaIngresoEmpleado" class="input100" onchange="changeCampos()">
                </div>
            </div>

            <div class="w3-row">
                <!-- -------------------- Apellido / Docimilio / Documento -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Categoria</span>
                    <select class="w3-select bg1" name="CategoriaEmpleado" id="CategoriaEmpleado">
                        <option value="0">< Seleccione una Calificacion Profesional ></option>
                        @{
                            if (ViewData["CalifProf"] != null)
                            {
                                IEnumerable<dynamic> CalifProf = (IEnumerable<dynamic>)ViewData["CalifProf"];

                                foreach (dynamic Loc in CalifProf)
                                {
                                    <option value="@Loc.Id">@Loc.Nombre</option>
                                }
                            }
                        }
                    </select>
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Total Remuneracion</span>
                    <input type="number" id="TotRemuneracionEmpleado" name="TotRemuneracionEmpleado" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Afiliado</span>
                    <select class="w3-select bg1" name="Afiliado" id="Afiliado">
                        <option value="NO">NO</option>
                        <option value="SI">SI</option>
                    </select>
                </div>
            </div>

            <div class="w3-row">
                <!-- -------------------- Apellido / Docimilio / Documento -------------------- -->
                <div class="w3-third w3-container wrap-input100 validate-input bg1 rs1-wrap-input100 alert-validate" data-validate="Enter Your Email (e@a.x)">
                    <span class="label-input100">Jornada</span>
                    <select class="w3-select bg1" name="JornadaEmpleado" id="JornadaEmpleado">
                        <option value="0"> < Seleccione una jornada > </option>
                        <option value="COMPLETA">JORNADA COMPLETA</option>
                        <option value="MEDIA">1/2 JORNADA</option>
                    </select>
                </div>
                <div class="w3-third ">
                    <button class="w3-right w3-margin-top enviar w3-card bg-zul w3-text-white w3-hover-green w3-hover-border-cyan" type="button" onclick="insertarEmpleado()">
                        Agregar Empleado
                    </button>
                </div>

                <div class="w3-third">

                </div>
            </div>

            <div class="w3-row">
                <div class="container">
                    <table id="tablaEmpleados">
                        <thead>
                            <tr>
                                <th>Apellido y Nombre</th>
                                <th>Cuil</th>
                                <th>Fecha Ingreso</th>
                                <th>Categoria</th>
                                <th>Remuneracion</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody id="bodyEmpleado">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        @*<br />
        <h3 class="h3-input100"> Antecedentes </h3>
        <hr class="hrs" />
